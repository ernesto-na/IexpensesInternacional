<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XxGamMaTypeRequestLovVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="xxgam.oracle.apps.xbol.maf.lov.server.XxGamMaTypeRequestLovVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT TipoSolicitud.lookup_code,
       TipoSolicitud.meaning,
       TipoSolicitud.description,
       TipoSolicitud.view_application_id,
       TipoSolicitud.lookup_type
FROM fnd_lookup_values TipoSolicitud
WHERE 1 = 1
  AND TipoSolicitud.LOOKUP_TYPE ='XXGAM_TYPE_REQUEST'
  AND TipoSolicitud.LANGUAGE = USERENV('LANG')
  AND TipoSolicitud.ENABLED_FLAG ='Y'
  AND TipoSolicitud.VIEW_APPLICATION_ID = 200
   ]]></SQLQuery>
