<ViewObject
   Name="XxGamMaCurrencyLovVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="xxgam.oracle.apps.xbol.maf.lov.server.XxGamMaCurrencyLovVORowImpl"
   ComponentClass="xxgam.oracle.apps.xbol.maf.lov.server.XxGamMaCurrencyLovVOImpl"
   ComponentInterface="xxgam.oracle.apps.xbol.maf.lov.server.common.XxGamMaCurrencyLovVO"
   ClientProxyName="xxgam.oracle.apps.xbol.maf.lov.server.client.XxGamMaCurrencyLovVOClient"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  currenciestl.currency_code CURRENCY_CODE,
        currenciestl.name CURRENCY_NAME,
        currenciestl.description CURRENCY_DESC
FROM fnd_currencies_tl currenciestl,
     fnd_currencies currencies
WHERE 1 = 1
  AND currenciestl.language = USERENV('LANG')
  AND currenciestl.CURRENCY_CODE = currencies.CURRENCY_CODE
  AND currencies.enabled_flag = 'Y'
  AND SYSDATE BETWEEN  NVL(start_date_active, sysdate) and nvl(end_date_active,sysdate+1) 
  AND currenciestl.currency_code in ('MXN','EUR','USD') /**Agregado para Pruebas**/
  ORDER BY currenciestl.name ASC
   ]]></SQLQuery>
