   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select fuser.user_id,
       fuser.user_name,
       people.person_id,
       people.employee_number,
       people.email_address,
       people.full_name,
       people.last_name,
       people.first_name,
       people.work_telephone,
       suppliers.vendor_type_lookup_code
from per_all_people_f people,
     ap_suppliers suppliers,
     fnd_user fuser
where 1 = 1
  and people.person_id = suppliers.employee_id  
  and people.person_id = fuser.employee_id
  and sysdate between people.effective_start_date and people.effective_end_date
   ]]></SQLQuery>
