   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="xxgam.oracle.apps.xbol.maf.lov.server.XxGamMaStatusNotificationTicketOfficeLovVOImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT StatusNotification.LOOKUP_CODE,
       StatusNotification.VIEW_APPLICATION_ID,
       StatusNotification.LOOKUP_TYPE,
       StatusNotification.MEANING,
       StatusNotification.DESCRIPTION,
       StatusNotification.LANGUAGE
FROM   FND_LOOKUP_VALUES StatusNotification 
WHERE 1 = 1
  AND StatusNotification.LOOKUP_TYPE = 'XXGAM_NOTIFICATION_REQ'
  AND StatusNotification.LANGUAGE= userenv('LANG')
  AND StatusNotification.ENABLED_FLAG = 'Y'
  AND StatusNotification.LOOKUP_CODE IN ('EMI', 'NOE', 'CAN')
   ]]></SQLQuery>
